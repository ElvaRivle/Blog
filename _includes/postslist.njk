<ul class="nav">
{% for post in postslist | reverse %}
  <li class="post">
    {% if metadata.config.showDateList %}<time datetime="{{ post.date | readableDateString }}">{{ post.date | readableDateString }}</time><span>&nbsp;&nbsp;-&nbsp;</span>{% endif %}
    <a class="title" href="{{ post.url | url }}">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a>
    |
    {{ post | readingTime }}
  </li>
{% endfor %}
</ul>